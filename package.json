  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev -H 0.0.0.0",
    "dev:socket": "node server/socket-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:socket\" --names \"Next.js,Socket\" --prefix-colors \"cyan,magenta\"",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=__tests__/.*\\.integration\\.test\\.(ts|tsx)$",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "clean": "rm -rf .next dist coverage",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
