{
  "name": "law-education-platform",
  "version": "1.1.7",
  "description": "AI-powered legal education platform with Socratic teaching methodology",
  "private": true,
  "keywords": [
    "legal-education",
    "ai",
    "socratic-method",
    "law",
    "teaching"
