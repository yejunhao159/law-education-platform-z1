            "type": "contractual",
            "category": "合同请求权",
            "elements": [
              {
                "name": "合同成立",
                "description": "当事人意思表示一致，合同依法成立",
                "required": true,
                "evidenceTypes": ["合同书", "要约承诺", "履行行为"]
              },
              {
                "name": "合同有效",
